{
  "plugins": [
    "add-js-css",
    "favicon",
    "insert-logo",
    "code",
    "collapsible-menu",
    "disqus-legacy",
    "youtube-placeholder",
    "codepen-placeholder",
    "codesandbox-placeholder",
    "redirect",
    "redirect-https",
    "analytics"
  ],
  "pluginsConfig": {
    "favicon": {
      "shortcut": "assets/images/learnjs.ico",
      "bookmark": "assets/images/learnjs-ico.png",
      "appleTouch": "assets/images/learnjs-ico.png",
        "appleTouchMore": {
            "120x120": "assets/images/learnjs-ico.png",
            "180x180": "assets/images/learnjs-ico.png"
        }
    },
    "insert-logo": {
      "url": "https://learnjs.today/assets/images/learnjs.png"
    },
    "add-js-css": {
      "css": [
        "assets/style.css"
      ]
    },
    "disqus-legacy": {
      "shortname": "learnjs-today",
      "wrapper": "<h2>ðŸ’¬ Comments & New Ideas:</h2>{{disqus}}",
      "exclude": [
        "README.md",
        "contents/codepen/README.md",
        "contents/codepen/html/README.md",
        "contents/codepen/css/README.md",
        "contents/codepen/js/README.md",
        "contents/git/README.md"
      ]
    },
    "codepen-placeholder": {
      "account": "marcopeg",
      "username": "Marco Pegoraro",
      "width": "100%",
      "height": "300px"
    },
    "codesandbox-placeholder": {
      "width": "100%",
      "height": "450px",
      "view": "split",
      "fontsize": "12"
    },
    "analytics": {
      "learnjs.today": [ "google", "UA-148196523-1" ]
    }
  }
}